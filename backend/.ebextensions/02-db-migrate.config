container_commands:
  01_migrate:
    command: "source /var/app/venv/*/bin/activate && python -m flask db upgrade"
    leader_only: true
  02_create_tables:
    command: "source /var/app/venv/*/bin/activate && python -c 'from app import app, db; app.app_context().push(); db.create_all()'"
    leader_only: true
